<p id="notice"><%= notice %></p>

<h1>Place To Eats</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Tel</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @place_to_eats.each do |place_to_eat| %>
      <tr>
        <td><%= place_to_eat.name %></td>
        <td><%= place_to_eat.address %></td>
        <td><%= place_to_eat.latitude %></td>
        <td><%= place_to_eat.longitude %></td>
        <td><%= place_to_eat.tel %></td>
        <td><%= place_to_eat.url %></td>
        <td><%= link_to 'Show', place_to_eat %></td>
        <td><%= link_to 'Edit', edit_place_to_eat_path(place_to_eat) %></td>
        <td><%= link_to 'Destroy', place_to_eat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Place To Eat', new_place_to_eat_path %>
